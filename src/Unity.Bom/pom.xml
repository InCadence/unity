<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<groupId>com.incadencecorp.unity</groupId>
	<artifactId>unity-bom</artifactId>
	<version>0.0.1-SNAPSHOT</version>
	<packaging>pom</packaging>
	<name>Unity.bom</name>
	<properties>
		<project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
		<unityVersion>0.0.1-SNAPSHOT</unityVersion>
		<unityRMIVersion>0.0.1-SNAPSHOT</unityRMIVersion>
		<unityRestServiceVersion>0.0.1-SNAPSHOT</unityRestServiceVersion>
		<unityCommonVersion>0.0.1-SNAPSHOT</unityCommonVersion>
		<unityLocalConnectorVersion>0.0.1-SNAPSHOT</unityLocalConnectorVersion>
		<unityRestConnectorVersion>0.0.1-SNAPSHOT</unityRestConnectorVersion>
		<unityRmiConnectorVersion>0.0.1-SNAPSHOT</unityRmiConnectorVersion>
	</properties>
	<dependencyManagement>
		<dependencies>
			<dependency>
				<groupId>com.incadencecorp.unity</groupId>
				<artifactId>unity-core</artifactId>
				<version>${unityVersion}</version>
			</dependency>
			<dependency>
				<groupId>com.incadencecorp.unity</groupId>
				<artifactId>unity-common</artifactId>
				<version>${unityCommonVersion}</version>
			</dependency>
			<dependency>
				<groupId>com.incadencecorp.unity</groupId>
				<artifactId>unity-common</artifactId>
				<version>${unityCommonVersion}</version>
				<type>test-jar</type>
			</dependency>
			<dependency>
				<groupId>com.incadencecorp.unity</groupId>
				<artifactId>unity-local-connector</artifactId>
				<version>${unityLocalConnectorVersion}</version>
			</dependency>
			<dependency>
				<groupId>com.incadencecorp.unity</groupId>
				<artifactId>unity-rest-service</artifactId>
				<version>${unityRestServiceVersion}</version>
			</dependency>
			<dependency>
				<groupId>com.incadencecorp.unity</groupId>
				<artifactId>unity-rest-connector</artifactId>
				<version>${unityRestConnectorVersion}</version>
			</dependency>
			<dependency>
				<groupId>com.incadencecorp.unity</groupId>
				<artifactId>unity-rmi-connector</artifactId>
				<version>${unityRmiConnectorVersion}</version>
			</dependency>
		</dependencies>
	</dependencyManagement>
	<repositories>
		<repository>
			<releases>
				<enabled>true</enabled>
				<updatePolicy>always</updatePolicy>
				<checksumPolicy>warn</checksumPolicy>
			</releases>
			<snapshots>
				<enabled>true</enabled>
				<updatePolicy>always</updatePolicy>
				<checksumPolicy>fail</checksumPolicy>
			</snapshots>
			<id>public</id>
			<url>http://10.15.0.70:8081/nexus/content/groups/public</url>
		</repository>
	</repositories>
	<distributionManagement>
		<snapshotRepository>
			<id>snapshots</id>
			<url>http://10.15.0.70:8081/nexus/content/repositories/snapshots</url>
		</snapshotRepository>
	</distributionManagement>
	<modules>
		<module>../Unity.Pom</module>
	</modules>
</project>